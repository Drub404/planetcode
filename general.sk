discord command .rob [<member>]:
	trigger:
		id of event-guild is "551225024393117706"
		set {_rob} to difference between {rob.%event-member%.lastused} and now
		if {_rob} is less than 6 hours:
			reply with "You can't rob someone right now! %nl% Come back in %difference between 6 hours and {_rob}%"
			stop
		else if arg-1 = event-member:
			reply with "You can't rob yourself, lol."
		else if arg-1 is not set:
			reply with "You need to specify a member to rob!"
			stop
		else if {points::%arg-1%} < 100:
			reply with "You can't rob %name of arg-1% because they got nothing in their wallet!"
			stop
		else if {points::%arg-1%} is not 0:
			set {rob.%event-member%.lastused} to now
			set {_fine} to a random integer between 750 and 1000
			chance of 60%:
				reply with "%mention tag of event-member%, oh no! You got caught by the Planet: Code Police (PCP) and you were fined %{_fine}%"
				subtract {_fine} from {points::%event-member%}
				add {_fine} to {points::%arg-1%}
				stop
			else:
				set {_robbed} to a random integer between 400 and 750
				subtract {_robbed} from {points::%arg-1%}
				add {_robbed} to {points::%event-member%}
				reply with "%mention tag of event-member%, you successfully stole %{_robbed}% points of %name of arg-1%!"
				stop
		else:
			make embed:
				set color of embed to dark blue, dark aqua, blue, aqua or white
				set the author info of the embed to author named "Oopsie! We ran into an error!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of embed to "It seems that you can't access this, or it doesn't exist."
				set {_embed} to the last made embed
				set the footer of embed to footer with text "Planet Code by Drub"
				set the timestamp of embed to now
				reply with {_embed}
				stop

