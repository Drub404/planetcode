discord command .mark [<text>] [<text>]:
	trigger:
		id of event-guild is "551225024393117706"
		if arg-1 is not set:
			reply with "Correct Usage: `.mark [watching/playing/listening] [your text]`"
		else if arg-2 is not set:
			reply with "Correct Usage: `.mark [watching/playing/listening] [your text]`"
		else if {in.use} is true:
			reply with "The bot is still displaying something from someone else!"
		else if arg-2 contains ".com", ".net", ".xyz" or "http":
			reply with "You're not allowed to advertise through me!"
		else if {in.use} is false:
			if arg-1 is "watching":
				if {points::%event-member%} >= 500:
					subtract 500 from {points::%event-member%}
					mark "Planet Code" as watching arg-2
					reply with "Successfully marked myself as ""Watching `%arg-2%`""!"
					set {in.use} to true
					wait 120 seconds
					set {in.use} to false
					mark "Planet Code" as watching {@default}
					stop
				else:
					reply with "You don't have enough points, either withdraw some or gather more money."
			else if arg-1 is "playing":
				if {points::%event-member%} >= 500:
					subtract 500 from {points::%event-member%}
					mark "Planet Code" as playing arg-2
					reply with "Successfully marked myself as ""Playing `%arg-2%`""!"
					set {in.use} to true
					wait 120 seconds
					set {in.use} to false
					mark "Planet Code" as watching {@default}
					stop
				else:
					reply with "You don't have enough points, either withdraw some or gather more money."
			else if arg-1 is "listening":
				if {points::%event-member%} >= 500:
					subtract 500 from {points::%event-member%}
					mark "Planet Code" as listening to arg-2
					reply with "Successfully marked myself as ""Listening to `%arg-2%`""!"
					set {in.use} to true
					wait 120 seconds
					set {in.use} to false
					mark "Planet Code" as watching {@default}
					stop
				else:
					reply with "You don't have enough points, either withdraw some or gather more money."
			else:
				make embed:
					set color of embed to dark blue, dark aqua, blue, aqua or white
					set the author info of the embed to author named "Oopsie! We ran into an error!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
					set the description of embed to "It seems that you can't access this, or it doesn't exist."
					set {_error} to the last made embed
					set the footer of embed to footer with text "Executor: %event-member%" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
					set the timestamp of embed to now
					reply with {_error}
		else:
			make embed:
				set color of embed to dark blue, dark aqua, blue, aqua or white
				set the author info of the embed to author named "Oopsie! We ran into an error!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of embed to "It seems that you can't access this, or it doesn't exist."
				set {_error} to the last made embed
				set the footer of embed to footer with text "Executor: %event-member%" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				reply with {_error}

