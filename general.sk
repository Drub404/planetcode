discord command .random password [<number>] [<text>]:
	trigger:
		id of event-guild is "551225024393117706"
		if arg-1 is not set:
			reply with "Correct usage: ``.random password [number] [yes/no]`` %nl% ``[number]`` is a number that decides the lenght of the password. %nl% ``[yes/no]`` is a question if you want special characters in the password or not."
		else if arg-2 is not set:
			reply with "Correct usage: ``.random password [number] [yes/no]`` %nl% ``[number]`` is a number that decides the lenght of the password. %nl% ``[yes/no]`` is a question if you want special characters in the password or not."
		else if arg-1 > 100:
			reply with "The max charachter lenght is 100."
		else if arg-2 is "yes":
			delete event-message
			set {_string1} to random arg-1 char string from charset `a-zA-Z0-9-/-@-=-~-:-+-(-)-[-]-&-^- `
			make embed:
				set color of embed to dark blue, dark aqua, blue, aqua or white
				set the author info of the embed to author named "Your password.." with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of embed to "%{_string1}%"
				set {_pass} to the last made embed
				set the footer of embed to footer with text "Executor: %event-member%" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
			send {_pass} to event-member with event-bot
			reply with "I have sent the random generated password to you."
		else if arg-2 is "no":
			delete event-message
			set {_string2} to random arg-1 char string from charset `a-zA-Z0-9`
			make embed:
				set color of embed to dark blue, dark aqua, blue, aqua or white
				set the author info of the embed to author named "Your password.." with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of embed to "%{_string2}%"
				set {_pass} to the last made embed
				set the footer of embed to footer with text "Executor: %event-member%" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
			send {_pass} to event-member with event-bot
			reply with "I have sent the random generated password to you."

discord command .tempchannel [<text>] [<number>]s:
	trigger:
		id of event-guild is "551225024393117706"
		if event-member has role named "Administrator":
			if arg-1 is not set:
				reply with "You need to enter the name of the channel!"
			else if arg-2 is not set:
				reply with "You need to enter the duration of the channel!"
			else:
				set {_countdown} to arg-2
				set {_string} to random 6 char string from charset `a-zA-Z0-9`
				create text channel:
					set the name of the channel to "%arg-1%-%{_string}%"
					set nsfw state of the channel to false
					set the topic of the channel to "Bot Testing"
				create the last made channel in event-guild with {@bot} and store it in {_channel}
				reply with "Successfully created the text channel <##%id of {_channel}%>!"
				loop arg-2 times:
					subtract 1 from {_countdown}
					wait 1 second
					if {_countdown} is 0:
						delete random channel out of {_channel}
		else:
			reply with ":x: | You can't create temporary channels yet!"

