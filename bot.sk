discord command .staff [<text>]:
	trigger:
		set {_staff} to difference between {staff.%event-member%.lastused} and now
		if {_staff} is less than 1 seconds:
			reply with "You can't request assistance from staff right now because you're on a cooldown for %difference between 1 seconds and {_staff}%!"
			stop
		else if arg-1 is not set:
			make embed:
				set color of embed to white
				set title of the embed to "Wait, so you need help but don't know why you need help? Hmm.. nope! %nl% *Pro tip: try using ``.staff I lost a role!`` instead!*"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_embed} to the last made embed
				reply with {_embed}
		else:
			make embed:
				set color of embed to white
				set title of the embed to "Someone needs YOUR help!"
				set description of the embed to "**Reason:** ```%arg-1%```%nl%**Member:** %event-member% %nl%**User ID:** %id of event-member% %nl%**Mention Tag:** %mention tag of event-member%"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_needhelp} to the last made embed
				#send "@here" to channel with id "561506865595285505" with event-bot
				send {_needhelp} to channel with id "561506865595285505" with event-bot and store the message in {needhelpemoji}
				add reactions "white_check_mark" to reactions of {needhelpemoji}
			make embed:
				set color of embed to white
				set title of the embed to "Successfully notified staff that you were in need of help!"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_embed} to the last made embed
				reply with {_embed}
				set {staff.%event-member%.lastused} to now

discord command .reward [<member>] [<integer>]:
	trigger:
		delete event-message
		set {_reward} to difference between {reward.%event-member%.lastused} and now
		if {_reward} is less than 5 seconds:
			reply with "You can't reward someone right now because you're on a cooldown for %difference between 5 seconds and {_reward}%!"
			stop
		else if event-member has role named "Competition Manager":
			if arg-1 is set:
				if arg-2 is set:
					add arg-2 to {points::%arg-1%}
					reply with "Successfully rewarded %arg-1% with %arg-2% points!"
					set {reward.%event-member%.lastused} to now
				else:
					reply with "You need to specify an amount!"
			else:
				reply with "You need to specify a member!"
		else:
			make embed:
				set color of embed to white
				set the author info of the embed to author named "Oopsie! We ran into an error!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of embed to "It seems that you can't access this, or it doesn't exist."
				set {_embed} to the last made embed
				set the footer of embed to footer with text "Planet Code by Drub"
				set the timestamp of embed to now
				reply with {_embed}

discord command .answer [<text>]:
	trigger:
		delete event-message
		if arg-1 is not set:
			make embed:
				set color of embed to white
				set title of the embed to "You can't answer nothing!"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_embed} to the last made embed
