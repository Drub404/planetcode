discord command .say [<text>]:
	trigger:
		set {_say} to difference between {say.%event-member%.lastused} and now
		if {_say} is less than 1 hour:
			reply with "You can't let the bot say something right now because you're on a cooldown for %difference between 1 hour and {_say}%!"
			stop
		if arg-1 is not set:
			reply with "Do you really want me to say nothing?"
			stop
		else if arg-1 contains "@everyone":
			reply with "Nope, not doing that!"
			stop
		else if arg-1 contains "@here":
			reply with "Nope, not doing that!"
			stop
		else:
			delete event-message
			reply with "%arg-1%"
			set {say.%event-member%.lastused} to now
			stop

discord command .status:
	roles: Founder
	trigger:
		loop 9999999999999999999999999999 times:
			mark "Planet Code" as playing "with your girl."
			set status of "Planet Code" to do not disturb
			wait 60 seconds
			mark "Planet Code" as watching "funny vines."
			set status of "Planet Code" to idle
			wait 60 seconds
			mark "Planet Code" as listening "Drub complaining."
			set status of "Planet Code" to online
			wait 60 seconds
			mark "Planet Code" as playing "a random game."
			set status of "Planet Code" to do not disturb
			wait 60 seconds
			mark "Planet Code" as watching "the stars."
			set status of "Planet Code" to idle
			wait 60 seconds
			mark "Planet Code" as listening "the radio."
			set status of "Planet Code" to online
			wait 60 seconds

discord command .reset [<member>]:
	trigger:
		set {feedback::%arg-1%} to false
		reply with "Done."

discord command .feedback [<integer>]:
	trigger:
		if arg-1 is not set:
			make embed:
				set color of embed to white
				set the author info of the embed to author named "Please enter an integer between 0 and 10!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of the embed to "``0`` = Very bad made bot, nothing works and everything is so ugly. %nl% ``10`` = Outstanding bot! Works flawlessly and found no bugs 'till now."
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_arg1} to the last made embed
				reply with {_arg1}
		else if {feedback::%event-member%} is true:
			make embed:
				set color of embed to white
				set the author info of the embed to author named "You have already rated the bot!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_alreadyvoted} to the last made embed
				reply with {_alreadyvoted}
		else if arg-1 is between 0 and 10:
			make embed:
				set color of embed to white
				set the author info of the embed to author named "Thanks for rating the Planet Code bot!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_vote} to the last made embed
				reply with {_vote}
				set {feedback::%event-member%} to true
			make embed:
				set color of embed to white
				set the author info of the embed to author named "New rate has came through!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set description of the embed to "User: %event-member% %nl% User ID: %id of event-member%"
				set title of the embed to "User has given feedback with number: ``%arg-1%``"
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				set {_embed} to the last made embed
				send {_embed} to user with id "257875009270317056" with event-bot and store the message in {_feedback}
				send {_feedback} to channel with id "561706661820235806" with event-bot
		else:
			make embed:
				set color of embed to white
				set the author info of the embed to author named "Oopsie! We ran into an error!" with no url and icon "https://cdn.discordapp.com/attachments/551227990155984905/555426102059859989/PlanetCode-pfp.png"
				set the description of embed to "It seems that you can't access this, or it doesn't exist."
				set {_error} to the last made embed
				set the footer of embed to footer with text "Planet Code by Drub" and icon "https://cdn.discordapp.com/avatars/257875009270317056/7480d9cc1f4928c09391f6ef69241431.png"
				set the timestamp of embed to now
				reply with {_error}


