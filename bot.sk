		else if arg-2 <= 0:
			reply with "You can't gamble a negative amount or gamble 0 points!"
		else if {points::%event-member%} >= arg-2:
			if arg-1 is "a":
				chance of 45%:
					subtract arg-2 from {points::%event-member%}
					add arg-2 to {points::%event-member%}
					add arg-2 to {points::%event-member%}
					reply with "You won! You now have %{points::%event-member%}% points."
					stop
				else:
					subtract arg-2 from {points::%event-member%}
					reply with "You lost. You now have %{points::%event-member%}% points."
					stop
			else if arg-1 is "b":
				chance of 25%:
					subtract arg-2 from {points::%event-member%}
					add arg-2 to {points::%event-member%}
					add arg-2 to {points::%event-member%}
					add arg-2 to {points::%event-member%}
					stop
				else:
					subtract arg-2 from {points::%event-member%}
					reply with "You lost. You now have %{points::%event-member%}% points."
					stop
			else if arg-1 is "c":
				chance of 15%:
					subtract arg-2 from {points::%event-member%}
					add arg-2 to {points::%event-member%}
					add arg-2 to {points::%event-member%}
					add arg-2 to {points::%event-member%}
					add arg-2 to {points::%event-member%}
					stop
				else:
					subtract arg-2 from {points::%event-member%}
					reply with "You lost. You now have %{points::%event-member%}% points."
					stop
			else:
				reply with "I don't recognize that one."
		else:
			reply with "You don't have that many points in your wallet!"

